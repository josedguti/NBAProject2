<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Team</th>
        <th>Conference</th>
        <th>Ranking</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% teams.forEach(function(t) { %>
        <tr>
          <td><%= t.teamname %></td>
          <td><%= t.conference %></td>
          <td><%= t.ranking %></td>
          <td><a href="/teams/<%= t._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
<%- include('../partials/footer') %>